.container {
  display flex
  margin-left 130px
  justify-content space-around
  @media (max-width: 1500px) {
    margin-left 0
  }
}

.seeMore {
  margin-right 10px
  margin-left 10px
  display flex
  justify-content center
  align-items center
  flex-direction column
  width 110px
  height 110px
  background-color rgba(255, 255, 255, 0.5)
  color #222222
  font-size 16px
  text-decoration none
  transition all 0.1s
  &:hover,
  &:active,
  &:focus {
    background-color #222222
    color #fefefe
  }
}

.webpages {
  display flex
  flex-wrap wrap
  justify-content center
  padding 0 20px
  @media (max-width: 1200px) {
    max-width 800px
  }
}
