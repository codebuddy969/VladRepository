{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "paths": {
      "@angular/*": [
        "../node_modules/@angular/*"
      ],
      "@nebular/*": [
        "../node_modules/@nebular/*"
      ],
      "@pages/*": [
        "./app/pages/*"
      ],
      "@member/*": [
        "./app/pages/member/*"
      ],
      "@partner/*": [
        "./app/pages/partner/*"
      ],
      "@core/*": [
        "./app/@core/*"
      ],
      "@theme/*": [
        "./app/@theme/*"
      ],
      "@commonComponents/*": [
        "./app/common/components/*"
      ]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "../node_modules/@nebular/**/*.spec.ts"
  ],
  "include": [
    "../src/*.ts",
    "../src/**/*.ts",
    "../node_modules/@nebular/**/*.ts"
  ]
}
